    "frets-styles-generator": "^2.2.1",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.0.1",
    "mocha": "^9.0.0",
    "postcss": "8.3.2",
    "prettier": "2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.6",
