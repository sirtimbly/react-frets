{
  "name": "react-frets",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "private": "true",
  "scripts": {
    "dev": "webpack serve --open",
    "build": "npm run styles && webpack",
    "lint": "prettier --check 'src/**/*.{js,ts,tsx,jsx}' && eslint --quiet --cache 'src/**/*.{js,ts,tsx}'",
    "lint:fix": "prettier --write 'src/**/*.{js,ts,tsx,jsx}' && eslint --quiet --cache --fix 'src/**/*.{js,ts,tsx}'",
    "styles": "tailwind build src/tailwind.css -o src/output.css && frets-styles-generator -r src/output.css",
    "watch:css": "nodemon -x npm run styles -w tailwind.config.js -w src/tailwind.css",
    "start": "run-p watch:css dev",
    "test": "TS_NODE_PROJECT=\"tsconfig.testing.json\" mocha -r ts-node/register tests/**.test.ts"
  },
  "engines": {
    "node": "14.x"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/chai": "^4.2.9",
    "@types/lodash": "^4.14.170",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.7",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "autoprefixer": "10.0.2",
    "chai": "^4.2.0",
    "esbuild-loader": "^2.13.1",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "frets-styles-generator": "^2.2.0",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.0.1",
    "mocha": "^9.0.0",
    "nodemon": "^2.0.2",
    "postcss": "8.3.2",
    "prettier": "2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "tailwindcss": "^2.1.4",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.6",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.2"
  },
