    "styles": "tailwind build src/tailwind.css -o src/output.css && frets-styles-generator -r src/output.css",
    "start": "npm run dev",
    "test": "TS_NODE_PROJECT=\"tsconfig.testing.json\" mocha -r ts-node/register tests/**.test.ts",
    "build:scripts": "esbuild scripts/dev.ts --platform=node --format=cjs --outfile=scripts/dev.js"
  },
  "engines": {
    "node": "14.x"
  },
  "keywords": [],
